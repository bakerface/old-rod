{
  "name": "old-rod",
  "version": "1.2.1",
  "description": "Catch errors in your express routes",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Chris Baker",
  "license": "MIT",
  "homepage": "https://github.com/bakerface/old-rod",
  "keywords": [
    "catch",
    "error",
    "express",
    "middleware",
    "route"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bakerface/old-rod.git"
  },
  "bugs": {
    "url": "https://github.com/bakerface/old-rod/issues"
  },
  "scripts": {
    "pretest": "xo",
    "test": "istanbul cover _mocha"
  },
  "xo": {
    "esnext": false,
    "space": true,
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it"
    ],
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "no-extend-native": [
        "off"
      ],
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "express": "^4.16.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0",
    "xo": "^0.22.0"
  }
}
